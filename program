#include<iostream>
using namespace std;
void check(int a[],int n,int k)
{
    int i,early=0,late=0;
    for(i=0;i<n;i++)
    {
        if(a[i]<=0)
        {
            early++;
        }
        else{
            late++;
        }
    }
    if(early==k || early>k)
    cout<<"NO\n";
    else
    cout<<"YES\n";
    
}
int main()
{
    int t;
    cin>>t;
    for(int j=0;j<t;j++)
    {
        int n,k;
        cin>>n>>k;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }   
        check(a,n,k);
    }
}
